    "release": "powershell -ExecutionPolicy Bypass -File ./build-release.ps1",
    "release:bat": "cmd /c build-release.bat"
  },
  "keywords": [
    "spt",
    "tarkov",
    "ai",
    "bots",
    "spawning",
    "live-tarkov",
    "bosses",
    "gear-progression",
    "sain",
    "fika",
    "bigbrain",
    "waypoints"
  ],
